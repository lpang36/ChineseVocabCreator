import java.util.*;
import java.lang.*;
import java.io.*;
import java.net.*;
import org.jsoup.*;

public class Parser {
  private static String getUrlSource(String url) {
    try {
      return Jsoup.parse(new URL(url).openStream(), "UTF-8", url).html();
      //return Jsoup.connect(url).get().html();
    } catch (IOException e) {
    }
    return null;
  }
  private boolean isChineseCharacter (char x) {
    return Character.isIdeographic(x);
  }
